@model OSPI.Infrastructure.Models.ElectionModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout_User_loggedIn.cshtml";
}
<div>
    <h4>eBallot Details</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RefCode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RefCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegStartDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegStartDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegEndDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegEndDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VotingStartDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VotingStartDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VotingEndDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VotingEndDate)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" class="btn btn-primary pull-left" asp-route-id="@Model.ElectionID"><i class="fa fa-edit" aria-hidden="true"></i> Edit</a>
    <a asp-action="Delete" class="btn btn-danger pull-left" asp-route-id="@Model.ElectionID"><i class="fa fa-trash" aria-hidden="true"></i>  Delete</a>
    <a asp-action="Index" class="btn btn-default pull-left"><i class="fa fa-list" aria-hidden="true"></i> Back</a>
</div>